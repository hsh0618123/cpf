/* header------------------------------------------- */
	.header {zoom*:1;width:100%;min-height:120px;box-sizing:border-box;padding:15px;position: fixed;z-index:9999;}
	.header:after{content:"";display:block;clear:both;}
	.header .logo{width:305px; margin:0 auto;}
	.header .menu{float:right;}
	.header .menu .mm{font-size:26px;width:150px;height:50px;background:red;color:#fff;line-height:50px;text-align: center;cursor:pointer;}
	.header .menu .gnb{float:right;width:150px;height:175px;background:rgba(255,255,255,0.8);box-sizing:border-box;padding:5px;display: none;}
	.header .menu .gnb li{margin-top:10px;}
	.header .menu .gnb li a{display: block;text-align: center;color:#abd1ec;}
