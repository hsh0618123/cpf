<!doctype html>
<html lang="en">
 <head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width,initial-scale=1.0,maximum-scale=1.0,minimum-scale=1.0,user-scalable=no">
  <title>한세현</title>
  <link rel="stylesheet" href="banila.css">
  <script src="http://code.jquery.com/jquery-latest.min.js"></script>
  <script type="text/javascript" src="banila.js"></script>
 </head>
 <body>
	<header class="header">
		<a href="#"><h1 class="logo"><img src="img/logo.png" width="268" height="58" border="0" alt="banila"></h1></a>
		<div class="menu">
			<p class="mm">Menu</p>
			<ul class="gnb">
				<li><a href="#">1</a></li>
				<li><a href="#">2</a></li>
				<li><a href="#">3</a></li>
				<li><a href="#">4</a></li>
				<li><a href="#">5</a></li>
			</ul>		
		</div>
	</header>
	<ul class="page">
		<li class="active"><a href="#">Main</a></li>
		<li><a href="#">1</a></li>
		<li><a href="#">2</a></li>
		<li><a href="#">3</a></li>
		<li><a href="#">4</a></li>
		<li><a href="#">5</a></li>
	</ul>
	<section class="section">
		<div class="center">
			<div class="se se_1">
				
				<div class="se_1_img">
					<div class="slideshow-container">

					<div class="mySlides fade">
					  <div class="numbertext">1 / 3</div>
					  <img src="img/se1_img.png" style="width:100%">
					  <div class="text">Caption Text</div>
					</div>

					<div class="mySlides fade">
					  <div class="numbertext">2 / 3</div>
					  <img src="img/se1_img.png" style="width:100%">
					  <div class="text">Caption Two</div>
					</div>

					<div class="mySlides fade">
					  <div class="numbertext">3 / 3</div>
					  <img src="img/se1_img.png" style="width:100%">
					  <div class="text">Caption Three</div>
					</div>

					<a class="prev" onclick="plusSlides(-1)">&#10094;</a>
					<a class="next" onclick="plusSlides(1)">&#10095;</a>

					</div>
					<div style="text-align:center">
					  <span class="dot" onclick="currentSlide(1)"></span> 
					  <span class="dot" onclick="currentSlide(2)"></span> 
					  <span class="dot" onclick="currentSlide(3)"></span> 
					</div>
				</div>
			</div>
			<div class="se se_2">
				<div class="se_2_img">
					<div class="container1">
					  <img src="img/se1_img.png" alt="Avatar" class="image">
					  <div class="overlay">
						<div class="text1">Hello World</div>
					  </div>
					</div>

					<div class="container1">
					  <img src="img/se1_img.png" alt="Avatar" class="image">
					  
					  <div class="overlay">
						<div class="text1"><img src="img/se2_img.png" alt="Avatar" class="image"></div>
					  </div>
					</div>
				</div>
			</div>
			<div class="se se_3">
				<div class="se_3_img">
					<div class="slideshow-container">

					<div class="mySlides fade">
					  <div class="numbertext">1 / 3</div>
					  <img src="img/se1_img.png" style="width:100%">
					  <div class="text">Caption Text</div>
					</div>

					<div class="mySlides fade">
					  <div class="numbertext">2 / 3</div>
					  <img src="img/se1_img.png" style="width:100%">
					  <div class="text">Caption Two</div>
					</div>

					<div class="mySlides fade">
					  <div class="numbertext">3 / 3</div>
					  <img src="img/se1_img.png" style="width:100%">
					  <div class="text">Caption Three</div>
					</div>

					<a class="prev" onclick="plusSlides(-1)">&#10094;</a>
					<a class="next" onclick="plusSlides(1)">&#10095;</a>

					</div>
					<div style="text-align:center">
					  <span class="dot" onclick="currentSlide(1)"></span> 
					  <span class="dot" onclick="currentSlide(2)"></span> 
					  <span class="dot" onclick="currentSlide(3)"></span> 
					</div>
				</div>
			</div>
			<div class="se se_4"></div>
			<div class="se se_5"></div>
			<div class="se se_6"></div>
		</div>
	</section>
 </body>
</html>
